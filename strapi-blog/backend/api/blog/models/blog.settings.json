{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "name": "Blog"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "blogTitle": {
      "type": "string",
      "required": true,
      "minLength": 5,
      "unique": true
    },
    "blogContent": {
      "type": "richtext"
    },
    "featuredImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "categories": {
      "via": "blogs",
      "collection": "categories"
    }
  }
}
